<h1><%= @list.name %> List</h1>
<ul>
    <% @list.items.each do |item| %>

<li><%= item.name %> | <%= link_to "Remove Item", find_listItem(item), method: :delete, data: { confirm: "Are you sure you want to remove this item?" } %></li>
    <% end %>
</ul>
<%= link_to "Add Items To Your List",  categories_path %> | <%= link_to "Go to all lists", user_path(@user) %>
